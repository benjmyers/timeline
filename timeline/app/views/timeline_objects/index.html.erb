<div class="span12 row-fluid">

<h2>Your Timelines</h2>

<% @timeline_objects.each do |timeline_object| %>
  <div class="span6 row-fluid">
      <blockquote>
        <p> <%= link_to timeline_object.title, timeline_object %></p>
        <small> <%= link_to 'Edit', edit_timeline_object_path(timeline_object) %> | <%= link_to 'Delete', timeline_object, method: :delete, data: { confirm: 'Are you sure?' } %> </td></small>
      </blockquote>
  </div>
<% end %>



  <div class="container span4" align="center">
  <%= link_to "New Timeline", new_timeline_object_path, class: "btn btn-success btn-large btn-block" %>
  </div>
</div>